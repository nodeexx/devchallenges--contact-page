@layer base {
  /* be-vietnam-pro-latin-100-normal */
  @font-face {
    font-family: 'Be Vietnam Pro';
    font-style: normal;
    font-display: swap;
    font-weight: 100;
    src:
      url(@fontsource/be-vietnam-pro/files/be-vietnam-pro-latin-100-normal.woff2)
        format('woff2'),
      url(@fontsource/be-vietnam-pro/files/be-vietnam-pro-latin-100-normal.woff)
        format('woff');
    unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA,
      U+02DC, U+0304, U+0308, U+0329, U+2000-206F, U+2074, U+20AC, U+2122,
      U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
  }

